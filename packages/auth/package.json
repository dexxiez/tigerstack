{
  "name": "@tigerstack/auth",
  "version": "0.0.0-alpha5",
  "type": "module",
  "private": false,
  "scripts": {
    "publish-pkg": "pnpm publish --access public"
  },
  "dependencies": {
    "axios": "1.7.9",
    "reflect-metadata": "0.2.2",
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "dependencies": {}
  },
  "devDependencies": {
    "@tigerstack/core": "workspace:*",
    "@tigerstack/http": "workspace:*"
  },
  "peerDependencies": {
    "@tigerstack/core": "0.0.0-alpha5",
    "@tigerstack/http": "0.0.0-alpha5"
  },
  "peerDependenciesMeta": {
    "@tigerstack/core": {
      "optional": false
    },
    "@tigerstack/http": {
      "optional": false
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./oauth": {
      "types": "./dist/oauth/index.d.ts",
      "import": "./dist/oauth/index.js",
      "require": "./dist/oauth/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "./oauth": [
        "./dist/oauth/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
