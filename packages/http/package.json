{
  "name": "@tigerstack/http",
  "version": "0.0.0-alpha2",
  "type": "module",
  "private": false,
  "scripts": {
    "publish": "pnpm publish --access public"
  },
  "dependencies": {
    "@tigerstack/core": "workspace:*",
    "reflect-metadata": "0.2.2",
    "tslib": "^2.3.0"
  },
  "publishConfig": {
    "dependencies": {
      "@tigerstack/core": "0.0.0-alpha2"
    }
  },
  "devDependencies": {
    "@types/koa": "2.15.0",
    "cli-table3": "0.6.5",
    "koa": "^2.15.3",
    "uuid": "11.0.5"
  },
  "peerDependencies": {
    "koa": "^2.15.3"
  },
  "peerDependenciesMeta": {
    "koa": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapters/koa": {
      "types": "./dist/adapters/koa/index.d.ts",
      "import": "./dist/adapters/koa/index.js",
      "require": "./dist/adapters/koa/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "adapters/koa": [
        "./dist/adapters/koa/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
