{
  "name": "@tigerstack/core",
  "version": "0.0.0-alpha3",
  "private": false,
  "type": "module",
  "scripts": {
    "publish": "pnpm publish --access public"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "reflect-metadata": "0.2.2",
    "tslib": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./internals": {
      "types": "./dist/internals/index.d.ts",
      "import": "./dist/internals/index.js",
      "require": "./dist/internals/index.cjs"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js",
      "require": "./dist/errors/index.cjs"
    },
    "./di": {
      "types": "./dist/di/index.d.ts",
      "import": "./dist/di/index.js",
      "require": "./dist/di/index.cjs"
    },
    "./logs": {
      "types": "./dist/logs/index.d.ts",
      "import": "./dist/logs/index.js",
      "require": "./dist/logs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "errors": [
        "./dist/errors/index.d.ts"
      ],
      "di": [
        "./dist/di/index.d.ts"
      ],
      "logs": [
        "./dist/logs/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
